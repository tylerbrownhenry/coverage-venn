{
  "name": "coverage-venn",
  "version": "1.0.0",
  "description": "Component coverage analysis and visualization tool",
  "main": "src/index.ts",
  "scripts": {
    "analyze": "ts-node scripts/analyze.ts",
    "migrate": "ts-node scripts/migrate.ts",
    "validate": "ts-node scripts/validate.ts",
    "tsc:init": "tsc --init",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "metro:start": "metro start",
    "metro:build": "metro build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:instrument": "node scripts/instrumentation/run.js",
    "test:instrument:watch": "node scripts/instrumentation/run.js --watch",
    "coverage": "node scripts/coverage.js",
    "coverage:analyze": "ts-node scripts/analysis/analyze/analyze-coverage.ts",
    "coverage:mock": "ts-node scripts/analysis/analyze/analyze-mock-app.ts",
    "report": "node scripts/report.js",
    "report:basic": "node scripts/report-html.js reports/mock-analysis.json coverage/html/report.html",
    "report:full": "node scripts/report.js --full",
    "report:export": "node scripts/report.js --export",
    "report:interactive": "node scripts/report.js --interactive",
    "report:correlated": "ts-node scripts/generate/generate-html-report-enhanced.ts",
    "scan": "ts-node scripts/scan.js",
    "scan:mock": "ts-node scripts/analysis/scan/scan-mock-app.ts",
    "analyze:app": "ts-node scripts/analysis/analyze/analyze-mock-app.ts",
    "analyze:mock": "ts-node scripts/analysis/analyze/analyze-mock-app.ts",
    "correlate:coverage": "ts-node scripts/analysis/correlate/correlate-coverage.ts",
    "correlate:mock": "COVERAGE_SOURCE=mock ts-node scripts/analysis/correlate/correlate-coverage.ts",
    "generate:testids": "ts-node scripts/generate/generate-test-ids.ts",
    "export:coverage": "ts-node scripts/export/export-coverage.ts",
    "check:plugin": "node scripts/analysis/check/check-plugin.js",
    "run:script": "node scripts/run-script.js",
    "coverage:correlate:mock": "npm run scan:mock && npm run analyze:mock && npm run correlate:mock && npm run report:correlated",
    "coverage:correlate:project": "npm run scan:project && npm run analyze:project && npm run correlate:project && npm run report:correlated:project",
    "scan:project": "PROJECT_ROOT=$PROJECT_ROOT SOURCE_DIR=$SOURCE_DIR ts-node scripts/analysis/scan/scan-project.ts",
    "analyze:project": "PROJECT_ROOT=$PROJECT_ROOT ts-node scripts/analysis/analyze/analyze-project.ts",
    "correlate:project": "PROJECT_ROOT=$PROJECT_ROOT COVERAGE_SOURCE=project ts-node scripts/analysis/correlate/correlate-coverage.ts",
    "report:correlated:project": "PROJECT_ROOT=$PROJECT_ROOT COVERAGE_SOURCE=project ts-node scripts/generate/generate-html-report-enhanced.ts",
    "project:coverage": "PROJECT_ROOT=/Users/tylerhenry/Documents/GitHub/jogger/jogger/frontend npm run coverage:correlate:project"
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/generator": "^7.x",
    "@babel/helper-plugin-utils": "^7.26.5",
    "@babel/parser": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@babel/types": "^7.22.5",
    "@react-native/metro-config": "^0.77.0",
    "@types/istanbul-lib-coverage": "^2.x",
    "@types/node": "^16.x",
    "@types/react": "^18.x",
    "istanbul-lib-coverage": "^3.x",
    "metro": "^0.77.0",
    "metro-react-native-babel-transformer": "^0.77.0",
    "node-fetch": "^2.6.7",
    "ts-node": "^10.x"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.0.0",
    "@types/babel__core": "^7.20.1",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/babel__traverse": "^7.20.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^5.x",
    "@typescript-eslint/parser": "^5.x",
    "babel-jest": "^29.5.0",
    "eslint": "^8.x",
    "eslint-import-resolver-typescript": "^3.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-jest": "^27.x",
    "glob": "^7.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.x",
    "prettier": "^2.x",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  }
}
